#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Webkoo/Client/deposs/node_modules/.pnpm/laravel-vite-plugin@1.3.0_v_108c311fe467c147d60aea8155225443/node_modules/laravel-vite-plugin/bin/node_modules:/mnt/d/Webkoo/Client/deposs/node_modules/.pnpm/laravel-vite-plugin@1.3.0_v_108c311fe467c147d60aea8155225443/node_modules/laravel-vite-plugin/node_modules:/mnt/d/Webkoo/Client/deposs/node_modules/.pnpm/laravel-vite-plugin@1.3.0_v_108c311fe467c147d60aea8155225443/node_modules:/mnt/d/Webkoo/Client/deposs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Webkoo/Client/deposs/node_modules/.pnpm/laravel-vite-plugin@1.3.0_v_108c311fe467c147d60aea8155225443/node_modules/laravel-vite-plugin/bin/node_modules:/mnt/d/Webkoo/Client/deposs/node_modules/.pnpm/laravel-vite-plugin@1.3.0_v_108c311fe467c147d60aea8155225443/node_modules/laravel-vite-plugin/node_modules:/mnt/d/Webkoo/Client/deposs/node_modules/.pnpm/laravel-vite-plugin@1.3.0_v_108c311fe467c147d60aea8155225443/node_modules:/mnt/d/Webkoo/Client/deposs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/clean.js" "$@"
else
  exec node  "$basedir/../../bin/clean.js" "$@"
fi
